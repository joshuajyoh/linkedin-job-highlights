{
    "manifest_version": 3,
    "name": "LInkedIn Jobs Extended",
    "version": "0.3.0",
    "permissions": [ "tabs" ],
    "content_scripts": [
        {
            "js": [ "scripts/content.js" ],
            "matches": [
                "https://www.linkedin.com/jobs/search/?currentJobId=*"
            ]
        }
    ],
    "background": {
        "service_worker": "scripts/service-worker.js"
    }
}